{
  "manifest_version": 3,
  "name": "MarcX",
  "version": "0.0.0",
  "description": "A bookmarks manager",
  "icons": {
    "16": "client/src/public/images/bookmark.png"
  },
  "background": {
    "service_worker": "./background.js"
  },
  "chrome_url_overrides": {
    "newtab": "client/dist/index.html"
  },
  "permissions": ["activeTab", "scripting"]
}
